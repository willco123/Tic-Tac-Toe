{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "double", { "avoidEscape": true }],
    "semi": [2, "always"],
    "comma-dangle": [2, "always-multiline"],
    "indent": "off",
    "space-before-function-paren": "off",
    "no-var": 0,
    "camelcase": "off",
    "no-void": "off"
  }
}
